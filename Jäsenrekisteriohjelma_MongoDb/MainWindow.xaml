<Window x:Class="Jäsenrekisteriohjelma_MongoDb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jäsenrekisteriohjelma_MongoDb"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Jäsenrekisteri" HorizontalAlignment="Center" Height="39" Margin="0,10,0,0" VerticalAlignment="Top" Width="216" FontSize="20" FontWeight="Bold"/>
        <Label x:Name="lblEtunimi" Content="Etunimi" HorizontalAlignment="Left" Height="33" Margin="79,128,0,0" VerticalAlignment="Top" Width="65"/>
        <Label x:Name="lblSukunimi" Content="Sukunimi" HorizontalAlignment="Left" Height="33" Margin="79,166,0,0" VerticalAlignment="Top" Width="65"/>
        <Label x:Name="lblOsoite" Content="Osoite" HorizontalAlignment="Left" Height="33" Margin="79,206,0,0" VerticalAlignment="Top" Width="65"/>
        <Label x:Name="lblPosti" Content="Postinumero" HorizontalAlignment="Left" Height="33" Margin="79,245,0,0" VerticalAlignment="Top" Width="79"/>
        <Label x:Name="lblPuhelin" Content="Puhelin" HorizontalAlignment="Left" Height="33" Margin="79,286,0,0" VerticalAlignment="Top" Width="65"/>
        <Label x:Name="lblEmail" Content="Sähköposti" HorizontalAlignment="Left" Height="33" Margin="79,324,0,0" VerticalAlignment="Top" Width="79"/>
        <Label x:Name="lblAika" Content="JäsenyydenAlkuPvm" HorizontalAlignment="Left" Height="33" Margin="79,359,0,0" VerticalAlignment="Top" Width="121"/>
        <TextBox x:Name="txbEtunimi" HorizontalAlignment="Left" Height="33" Margin="223,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <TextBox x:Name="txbSukunimi" HorizontalAlignment="Left" Height="33" Margin="223,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <TextBox x:Name="txbOsoite" HorizontalAlignment="Left" Height="33" Margin="223,204,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <TextBox x:Name="txbPosti" HorizontalAlignment="Left" Height="33" Margin="223,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <TextBox x:Name="txbPuhelin" HorizontalAlignment="Left" Height="33" Margin="223,284,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <TextBox x:Name="txbEmail" HorizontalAlignment="Left" Height="33" Margin="223,322,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <Button x:Name="btn_Talenna" Content="Talenna" HorizontalAlignment="Left" Height="36" Margin="543,214,0,0" VerticalAlignment="Top" Width="92" Click="btn_Talenna_Click" RenderTransformOrigin="0.152,1.099"/>
        <Button x:Name="btn_Hae" Content="Hae" HorizontalAlignment="Left" Height="36" Margin="543,62,0,0" VerticalAlignment="Top" Width="93" Click="btn_Hae_Click" FontWeight="Bold" Background="#FFF5EFDB"/>
        <Button x:Name="btn_Päivitä" Content="Päivitä" HorizontalAlignment="Left" Height="36" Margin="674,214,0,0" VerticalAlignment="Top" Width="92" Click="btn_Päivitä_Click"/>
        <Button x:Name="btn_Poista" Content="Poista" HorizontalAlignment="Left" Height="36" Margin="673,62,0,0" VerticalAlignment="Top" Width="93" Click="btn_Poista_Click" FontWeight="Bold" Background="#FFF5EFDB"/>
        <DatePicker x:Name="datepicker_Aika" HorizontalAlignment="Left" Height="32" Margin="223,360,0,0" VerticalAlignment="Top" Width="232"/>
        <Label x:Name="lbl_searchBox" Content="Hakukentä" HorizontalAlignment="Left" Height="33" Margin="42,65,0,0" VerticalAlignment="Top" Width="76" Background="White"/>
        <TextBox x:Name="txb_searchBox" HorizontalAlignment="Left" Height="33" Margin="140,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="315" Background="#FFF5EFDB"/>

    </Grid>
</Window>
